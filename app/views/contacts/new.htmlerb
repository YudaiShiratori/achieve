<%= form_with(model:@contact, local:true) do |form| %>
  <div class="name">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="email">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
